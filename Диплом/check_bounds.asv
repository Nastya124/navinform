function X_checked = check_bounds(X, map_size)
%     X(X <= 1) = 1;
%     X(X > map_size) = map_size;
%     X_checked = X;
L = length(X);
for i = 1:L
    if ((X(1,i)<=1)||(X(2,i)<=1)||(X(1,i)> map_size)||(X(2,i) > map_size))
        X(:,i)=[];
        L = L-1;
        i=i-1;
    end
    X_checked = X;
end
end